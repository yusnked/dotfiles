[alias]
	a   = add
	aa  = add -A
	ai  = add -i
	ap  = add -p
	au  = add -u
	b   = branch
	c   = commit
	ca  = commit --amend
	can = commit --amend --no-edit
	cm  = commit -m
	d   = diff
	ds  = diff --staged
	l   = log --graph --pretty=format:'%C(yellow)%h%Creset%Cred%d%Creset %Cgreen[%ad] %C(bold blue)<%an>%Creset%n %s%n' --date=format:"%y/%m/%d" --all --abbrev-commit
	lo  = log --graph --pretty=format:'%C(yellow)%h%Creset -%Cred%d%Creset %s %Cgreen[%ad] %C(bold blue)<%an>%Creset' --date=format:"%y/%m/%d" --all --abbrev-commit
	r   = restore
	re  = reset
	rea = reset HEAD
	reh = reset --hard
	res = reset --soft
	rl  = reflog
	rs  = restore --source
	s   = status -sb
	sw  = switch
	swc = switch -c
	x   = commit --no-gpg-sign
	xa  = commit --no-gpg-sign --amend
	xan = commit --no-gpg-sign --amend --no-edit
	xm  = commit --no-gpg-sign -m
[color]
	ui = true
[commit]
	gpgsign = true
[core]
	autocrlf  = false
	safecrlf  = true
	quotepath = false
[credential]
	helper = store
[init]
	defaultBranch = main
[log]
	date = iso
[pull]
	ff     = only
	rebase = false
[push]
	default = current
