# shellcheck disable=SC2168
local copied_pipestatus=("${pipestatus[@]}")

if [[ ${#copied_pipestatus[@]} -ge 2 ||
    (${copied_pipestatus[1]} -ge 129 && ${copied_pipestatus[1]} -le 159) ]]; then
    autoload -Uz _prompt-pipestatus
    _prompt-pipestatus "${copied_pipestatus[@]}"
else
    _prompt_pipestatus_var=''
fi
